<div>
  <table class="table-dashboard">
    <tr class="header">
      <td colspan="7">PARTIDOS PARA HOY - RONDA DE ELIMINACIÃ“N</td>
    </tr>
    <tr class="sub-header">
      <td>Nro</td>
      <td>Partido</th>
      <td>Ciudad</td>
      <td>Fecha</td>
      <td>Hora</th>
    </tr>
    <% elimination_matches.each do |match| %>
      <% match.get_teams  %>
      <tr class="row-game <%= cycle("odd", "even") %>">
        <td><%= match.match_number %></td>
        <td>
          <span><%= match.local_team.abbr %></span>
            <%= flag(match.local_team) %>
          <span><%= match.score_to_s %></span>
            <%= flag(match.visitor_team) %>
          <span><%= match.visitor_team.abbr %></span>
        </td>
        <td><%= match.stadium %></td>
        <td><%= match.date.strftime("%d-%m") %></td>
        <td><%= match.hour %></td>
      </tr>
    <% end %>
  </table>
</div>